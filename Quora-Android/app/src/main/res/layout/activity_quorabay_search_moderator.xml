<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".quorabay.QuorabaySearchModeratorActivity"
    android:orientation="vertical">
    <com.arlib.floatingsearchview.FloatingSearchView
        android:id="@+id/fsv_quorabay_searchModerator_floatingSearch"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:floatingSearch_searchBarMarginLeft="10dp"
        app:floatingSearch_searchBarMarginTop="10sp"
        app:floatingSearch_searchBarMarginRight="10sp"
        app:floatingSearch_searchHint="Search..."
        app:floatingSearch_suggestionsListAnimDuration="250"
        app:floatingSearch_showSearchKey="false"
        app:floatingSearch_leftActionMode="showHamburger"
        app:floatingSearch_close_search_on_keyboard_dismiss="true"
        tools:ignore="MissingConstraints">
    </com.arlib.floatingsearchview.FloatingSearchView>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/searchModerator_RecyclerView"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        android:layout_marginTop="30sp"
        android:layout_marginLeft="10sp"
        android:layout_marginRight="10dp"
        tools:ignore="MissingConstraints">
    </androidx.recyclerview.widget.RecyclerView>

</LinearLayout>